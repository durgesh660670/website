  </section>

  {{!-- banner1 section end --}}


  {{!-- services section start --}}


  <section id="services_section" class='py-3'>
    <div class="container-fluid">


      <div class="container text-center text-secondary">

        <h3>Services that we provide</h3>
      </div>

      <div class="row">
        {{#each services}}

        <div class="col-md-4">

          <div class="card mt-3">
            <div class="card-body">
              <div class="container text-center text-secondary">
                <i class="{{this.icon}} service-icon"></i>
              </div>

              <h3>{{this.title}}</h3>
              <p>{{this.description}}</p>
              <a href="{{this.link}}">{{this.linkText}}</a>
              <a href="#">Support</a>
            </div>
          </div>

        </div>
        {{/each}}
      </div>

    </div>
  </section>



  {{!-- services section end --}}


